{
  "plugins": [],
  "typescript": {
    "moduleRoot": "src"
  },
  "source": {
    "include": ["dist"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ".+\\.(test|spec).js"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "doc",
    "recurse": true,
    "readme": "README.md"
  }
}